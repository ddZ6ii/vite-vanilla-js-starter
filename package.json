{
  "name": "vite-vanilla-js-starter",
  "private": true,
  "version": "0.0.3",
  "description": "vite starter for html | css | vanilla js project",
  "author": "Denis Dezest <denis.dezest@gmail.com>",
  "repository": "git@github.com:ddZ6ii/vite-vanilla-js-starter.git",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "format:lint": "yarn run format && yarn run lint",
    "build": "yarn run lint && vite build",
    "preview": "vite preview --host --open",
    "build:preview": "yarn run build && yarn run preview"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "path": "^0.12.7",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "url": "^0.11.3",
    "vite": "^5.1.4",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-purgecss-updated-v5": "^1.2.0"
  }
}
